#: validators.py:31
#, python-format
msgid "Invalid Length (%s)"
msgstr "Hatalı Uzunluk (%s)"

#: validators.py:41
#, python-format
msgid "Must be %s characters or more"
msgstr "%s karakter ya da daha fazla olmalı"

#: validators.py:45
#, python-format
msgid "Must be %s characters or less"
msgstr "%s karakter ya da daha az olmalı"

#: validators.py:49
#, python-format
msgid "Must be more complex (%s)"
msgstr "Daha karmaşık olmalı  (%s)"

#: validators.py:77
#, python-format
msgid "Must contain %(UPPER)s or more unique uppercase characters"
msgstr "%(UPPER)s ya da fazla eşsiz büyük harf içermeli"

#: validators.py:81
#, python-format
msgid "Must contain %(LOWER)s or more unique lowercase characters"
msgstr "%(LOWER)s ya da fazla eşsiz küçük harf içermeli"

#: validators.py:85
#, python-format
msgid "Must contain %(DIGITS)s or more unique digits"
msgstr "%(DIGITS)s ya da fazla eşsiz sayı içermeli"

#: validators.py:89
#, python-format
msgid "Must contain %(PUNCTUATION)s or more unique punctuation characters"
msgstr "%(PUNCTUATION)s ya da fazla noktalama işareti içermeli"

#: validators.py:93
#, python-format
msgid "Must contain %(SPECIAL)s or more unique special characters"
msgstr "%(SPECIAL)s ya da fazla eşsiz özel karakter içermeli"

#: validators.py:97
#, python-format
msgid "Must contain %(WORDS)s or more unique words"
msgstr "%(WORDS)s ya da fazla eşsiz kelime içermeli"

#: validators.py:102
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Çok benzer [%(haystacks)s]"

#: validators.py:138
msgid "Based on a dictionary word"
msgstr "Sözlük bulunan kelime"

#: validators.py:154
msgid "Based on a common sequence of characters"
msgstr "Çok tipik bir karakter dizisi"
