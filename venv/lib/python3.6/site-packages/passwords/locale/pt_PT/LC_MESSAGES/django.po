#: fields.py:27
#, python-format
msgid "%i to %i characters"
msgstr "%i a %i carácteres"

#: fields.py:31
#, python-format
msgid "%i characters minimum"
msgstr "%i carácteres minímos"

#: validators.py:47
#, python-format
msgid "Invalid Length (%s)"
msgstr "Tamanho Inválido: %s"

#: validators.py:57
#, python-format
msgid "Must be %s characters or more"
msgstr "Deve ser %s carácter(es) ou mais"

#: validators.py:59
#, python-format
msgid "Must be %s characters or less"
msgstr "Deve ser %s carácter(es) ou menos"

#: validators.py:66
#, python-format
msgid "Must be more complex (%s)"
msgstr "Deve ser mais complexa (%s)"

#: validators.py:98
#, python-format
msgid "%(UPPER)s or more unique uppercase characters"
msgstr "deve conter %(UPPER)s ou carácteres maiúsculos mais únicos"

#: validators.py:102
#, python-format
msgid "%(LOWER)s or more unique lowercase characters"
msgstr "deve conter %(LOWER)s ou carácteres minúsculos mais únicos"

#: validators.py:106
#, python-format
msgid "%(LETTERS)s or more unique letters"
msgstr "deve conter %(LETTERS)s ou letras mais únicas"

#: validators.py:110
#, python-format
msgid "%(DIGITS)s or more unique digits"
msgstr "deve conter %(DIGITS)s ou dígitos mais únicos"

#: validators.py:114
#, python-format
msgid "%(PUNCTUATION)s or more unique punctuation characters"
msgstr "deve conter %(PUNCTUATION)s ou carácteres de pontuação mais únicos"

#: validators.py:118
#, python-format
msgid "%(SPECIAL)s or more unique special characters"
msgstr "deve conter %(SPECIAL)s ou carácteres especiais mais únicos"

#: validators.py:122
#, python-format
msgid "%(WORDS)s or more unique words"
msgstr "deve conter %(WORDS)s ou palavras mais únicas"

#: validators.py:119
msgid "must contain "
msgstr " "

#: validators.py:132
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Muito Similar a [%(haystacks)s]"

#: validators.py:176
msgid "Based on a dictionary word"
msgstr "Baseada numa palavra de dicionário"

#: validators.py:195
msgid "Based on a common sequence of characters"
msgstr "Baseada numa sequência comum de carácteres"
