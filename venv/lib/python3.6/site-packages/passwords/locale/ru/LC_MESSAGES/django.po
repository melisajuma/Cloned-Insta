#: fields.py:27
#, python-format
msgid "%i to %i characters"
msgstr "От %i до %i символов"

#: fields.py:31
#, python-format
msgid "%i characters minimum"
msgstr "Минимум %i символов"

#: validators.py:47
#, python-format
msgid "Invalid Length (%s)"
msgstr "Неправильная длина: %s"

#: validators.py:57
#, python-format
msgid "Must be %s characters or more"
msgstr "должна составлять %s символов или более"

#: validators.py:59
#, python-format
msgid "Must be %s characters or less"
msgstr "должна составлять %s символов или менее"

#: validators.py:66
#, python-format
msgid "Must be more complex (%s)"
msgstr "Должен быть более сложным: %s"

#: validators.py:98
#, python-format
msgid "%(UPPER)s or more unique uppercase characters"
msgstr "должен содержать %(UPPER)s или более уникальных прописных букв"

#: validators.py:102
#, python-format
msgid "%(LOWER)s or more unique lowercase characters"
msgstr "должен содержать %(LOWER)s или более уникальных строчных букв"

#: validators.py:106
#, python-format
msgid "%(LETTERS)s or more unique letters"
msgstr "должен содержать %(LETTERS)s или более уникальных букв"

#: validators.py:110
#, python-format
msgid "%(DIGITS)s or more unique digits"
msgstr "должен содержать %(DIGITS)s или более уникальных цифр"

#: validators.py:114
#, python-format
msgid "%(PUNCTUATION)s or more unique punctuation characters: %%s"
msgstr ""
"должен содержать %(PUNCTUATION)s или более уникальных знаков пунктуации: %%s"

#: validators.py:118
#, python-format
msgid "%(SPECIAL)s or more non unique special characters"
msgstr "должен содержать %(SPECIAL)s или более уникальных специальных символов"

#: validators.py:122
#, python-format
msgid "%(WORDS)s or more unique words"
msgstr "должен содержать %(WORDS)s или более уникальных слов"

#: validators.py:119
msgid "must contain "
msgstr " "

#: validators.py:132
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Слишком похож на [%(haystacks)s]"

#: validators.py:176
msgid "Based on a dictionary word"
msgstr "Основан на слове из словаря"

#: validators.py:195
msgid "Based on a common sequence of characters"
msgstr "Основан на распространенной последовательности символов"
