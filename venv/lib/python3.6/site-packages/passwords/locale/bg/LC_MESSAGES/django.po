#: fields.py:27
#, python-format
msgid "%i to %i characters"
msgstr "От %i до %i символа"

#: fields.py:31
#, python-format
msgid "%i characters minimum"
msgstr "Минимум %i символа"

#: validators.py:47
#, python-format
msgid "Invalid Length (%s)"
msgstr "Невалидна дължина (%s)"

#: validators.py:57
#, python-format
msgid "Must be %s characters or more"
msgstr "Трябва да бъде поне %s символа"

#: validators.py:59
#, python-format
msgid "Must be %s characters or less"
msgstr "Трябва да бъде не повече от %s символа"

#: validators.py:66
#, python-format
msgid "Must be more complex (%s)"
msgstr "Трябва да бъде по-сложна (%s)"

#: validators.py:98
#, python-format
msgid "%(UPPER)s or more unique uppercase characters"
msgstr "минимум %(UPPER)s главни букви"

#: validators.py:102
#, python-format
msgid "%(LOWER)s or more unique lowercase characters"
msgstr "минимум %(LOWER)s малки букви"

#: validators.py:106
#, python-format
msgid "%(LETTERS)s or more unique letters"
msgstr "минимум %(LETTERS)s уникални букви"

#: validators.py:110
#, python-format
msgid "%(DIGITS)s or more unique digits"
msgstr "минимум %(DIGITS)s уникални цифри"

#: validators.py:114
#, python-format
msgid "%(PUNCTUATION)s or more unique punctuation characters: %%s"
msgstr "минимум %(PUNCTUATION)s уникални пунктоационни знаци"

#: validators.py:118
#, python-format
msgid "%(SPECIAL)s or more non unique special characters"
msgstr "минимум %(SPECIAL)s уникални специални символи"

#: validators.py:122
#, python-format
msgid "%(WORDS)s or more unique words"
msgstr "минимум %(WORDS)s уникални думи"

#: validators.py:126
msgid "must contain "
msgstr "трябва да съдържа "

#: validators.py:132
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Много прилича на [%(haystacks)s]"

#: validators.py:176
msgid "Based on a dictionary word"
msgstr "Често срещана парола"

#: validators.py:195
msgid "Based on a common sequence of characters"
msgstr "Често срещана поредност на символи"
